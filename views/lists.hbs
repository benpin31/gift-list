<div id="user-lists">
    {{#each lists}} 
    <div class="list-container">
        <a href="/lists/{{this._id}}">{{this.name}}</a>
        <div id="event-crud">
            <a href="/lists/update/{{this._id}}"><i class="fas fa-edit" style="color:blue"></i></a>
            <a href="/lists/delete/{{this._id}}"><i class="fas fa-trash-alt red"></i></a>
        </div>
        {{!-- <i class="fas fa-trash-alt red"><a href="/lists/delete/{{this._id}}" class="crud-a"></a></i> --}}
        
    </div>
    {{/each}}
    <div class="create_list">
        <a href="/lists/create">Create a new list</a>
    </div>

</div>