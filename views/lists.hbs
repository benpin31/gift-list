<div id="user-lists">
    {{#each lists}} 
    <div class="list-container">
        <a href="/lists/{{this._id}}">{{this.name}}</a>
        <i class="fas fa-trash-alt red"><a href="/lists/delete/{{this._id}}"></a></i>
        
    </div>
    {{/each}}
    <div class="create_list">
        <a href="/lists/create">Create a new list</a>
    </div>

</div>