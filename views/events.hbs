<div id="user-lists">
    {{#each event}} 
    <div class="list-container">
        <a href="/events/{{this._id}}">{{this.name}}</a>
        <a href="/events/delete/{{this._id}}">Delete the event</a>
    </div>
    {{/each}}
    <div class="list-container">
        <a href="/events/create">Create new event</a>
    </div>

</div>