<div id="user-lists">
    {{#each lists}} 
    <div class="list-container">
        <a href="/lists/{{this._id}}">{{this.name}}</a>
        <a href="/lists/delete/{{this._id}}">Delete the list</a>
    </div>
    {{/each}}
    <div class="list-container">
        <a href="/lists/create">Create new list</a>
    </div>

</div>